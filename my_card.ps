    x12 y12 x22 y22 x3 y3 curveto
} def

/OutOfOutlineProjective {
    newpath
    0 1 0 1 moveto translate
    0 1 67 {
        10 0 5 5 lineto lineto
        .5 .8 .1 setrgbcolor
        stroke
        0 6 0 6 moveto translate
    } for
    newpath
    .5 1 .5 1 moveto translate
    0 1 44 {
        0 10 5 5 lineto lineto
        1 .1 1 setrgbcolor
        stroke
        6 0 6 0 moveto translate
    } for
    newpath
    -1 10 -1 10 moveto translate
    0 1 67 {
        10 0 5 -5 lineto lineto
        .5 .8 .1 setrgbcolor
        stroke
        0 -6 0 -6 moveto translate
    } for
    newpath
    10 -1 10 -1 moveto translate
    0 1 44 {
        0 -10 -5 -5 lineto lineto
        1 .1 1 setrgbcolor
        stroke
        -6 0 -6 0 moveto translate
    } for
} def

/QuadraticCurves {
    {
        /epsln exch def
        /sw 0 def
        newpath
        0 1 360 {
            /arg exch def
            /denom 1 epsln arg cos mul add def
            denom 0 ne { /r 20 denom div def
            /x r arg cos mul def
            /y r arg sin mul def
            arg 90 div setlinewidth
            sw 0 eq { x y moveto /sw 1 def }{ x y lineto } ifelse
            } { /r 0 def } ifelse
        } for
        stroke
    } for
} def

/QuadraticCurves2 {
    {
        /epsln exch def
        /sw 0 def
        newpath
        220 1 510 {
            /arg exch def
            /denom 1 epsln arg cos mul add def
            denom 0 ne { /r 20 denom div def
            /x r 35 add arg cos mul 45 epsln div sub def
            /y r 25 add arg sin mul def
            randomwidth
            sw 0 eq { x y moveto /sw 1 def }{ x y lineto } ifelse
            } { /r 0 def } ifelse
        } for
        stroke
    } for
} def

